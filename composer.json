{
    "name": "cooper/laravel-carbon-chinese-macros",
    "description": "Laravel Carbon Chinese Macros",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0 || ^9.0",
        "ext-calendar": "*"
    },
    "require-dev": {
        "orchestra/testbench": "6.*"
    },
    "authors": [
        {
            "name": "cooper",
            "email": "myxiaoao@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cooper\\CarbonChineseMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cooper\\CarbonChineseMacros\\Test\\": "tests",
            "Cooper\\CarbonChineseMacros\\Command\\": "command"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "generate-ide-helpers": "./cmd generate-ide-helpers"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cooper\\CarbonChineseMacros\\CarbonChineseMacrosServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
